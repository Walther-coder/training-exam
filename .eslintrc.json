{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "import/no-dynamic-require": 0,
             "global-require": 0,
             "strict": 0,
             "no-console": 0,
            "no-unused-vars": 0,
             "max-len": 0
    }
}
